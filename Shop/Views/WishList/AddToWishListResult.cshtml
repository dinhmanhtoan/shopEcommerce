@model AddToWishListResult

<div class="modal-header">
    <h4 class="modal-title" id="myModalLabel">@Model.Message</h4>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-md-3">
            <img alt="@Model.Item.ProductName" class="img-fluid" src="@Model.Item.ProductImage">
        </div>
        <div class="col-md-9">
            <h4>@Model.Item.ProductName</h4>
        </div>
    </div>
</div>
<div class="modal-footer">
    <a href="~/user/wishlist" type="button" class="btn btn-success">View Wish List</a>
</div>
